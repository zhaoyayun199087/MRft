<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:test="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/gao80">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:orientation="vertical">

        <com.mingri.future.airfresh.view.SlideUnlockView
            android:id="@+id/sv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            test:slideUnlockBackgroundResource="@mipmap/lock_bg"
            test:slideUnlockBlockResource="@mipmap/guanj_btn_diany" />


        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sv"
            android:id="@+id/ib_cancel"
            android:layout_marginTop="80dp"
            android:background="@null"
            android:src="@mipmap/guanj_btn_guanb" />
    </LinearLayout>
</RelativeLayout>
